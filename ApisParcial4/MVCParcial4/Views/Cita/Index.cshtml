@model IEnumerable<ApisParcial4.Models.Cita>

@{
    ViewBag.Title = "Lista de Citas";
}

<h2>Lista de Citas</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdCita)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdPaciente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdClinica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaCita)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoraCita)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdCita)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdPaciente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdClinica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaCita)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HoraCita)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.IdCita }) |
            @*@Html.ActionLink("Details", "Details", new { id = item.IdCita })*@ |
            @Html.ActionLink("Delete", "Delete", new { id = item.IdCita  })
        </td>
    </tr>
}

</table>
